}

div.sheetContainer {
/*	width: 100%;*/
	position: relative;
}

div.hPaneSeparator {
	position: absolute;
	height: 2px;
	font-size: 0px;
	overflow: hidden;
	width: 100%;
	left: 0px;
	border-top: 1px solid #8a8a8a;
	z-index: 20003000;
}

div.vPaneSeparator {
	position: absolute;
	height: 100%;
	width: 2px;
	top: 0px;
	border-left: 1px solid #8a8a8a;
	z-index: 20003000;
}

div.sheetPane {
	overflow: hidden;
	position: absolute;
	border: 0 solid #8a8a8a;
	background-color: white;
	cursor: default;
}

div.sheetPane div, div.sheetPane textarea {
	margin: 0;
	padding-left: 2px;
	font: 10pt Arial;
	position: absolute;
	overflow: hidden;
}
div.sheetPane textarea {
	border: 0;
	background-color: #fff0f0;
	padding: 0px;
	z-index: 20000006;
}
div.sheetPane div {
	-moz-user-select: none;
	-khtml-user-select: none;
	user-select: none;
}
div.sheetPane div.cell {
	padding-right: 2px;
	overflow: hidden;
	white-space: nowrap;
}
div.ShowGridLine div.sheetPane div.cell {
	border: 0 solid #dddddd;
	border-top-width: 1px;
	border-left-width: 1px;
}
div.sheetPane div.cell a {
	text-decoration: underline;
}
div.CellOverflow div.sheetPane div.cell {
	overflow: visible;
}
div.CellOverflowIE div.sheetPane div.cell {
	overflow-x: visible;
	overflow-y: hidden;
}
div.sheetPane div.rangeBoxContainer {
	z-index: 20000001;
	overflow: visible;
}
div.sheetPane div.cursor, div.sheetPane div.RTU,
div.sheetPane div.copy, div.sheetPane div.selector,
div.sheetPane div.filler, div.sheetPane div.lock, div.sheetPane div.myLock,
div.sheetPane div.namedRangeOriginBox,
div.sheetPane div.focusBox {
	z-index: 20000002;
	margin: -2px -1px -1px -2px;
}
div.sheetPane div.cursor {
	padding: 0px;
	background-color: #E9EffB;
	border: 2px solid #3E62A8;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=40);
	opacity: 0.40;
	-moz-opacity: 0.40;
	z-index: 20000000;
}
div.sheetPane div.cursorDot {
	background-color: #3E62A8;
/*	border: 2px solid #eff4f9; */
	width: 7px;
	height: 7px;
	z-index: 20000005;
	padding: 0px;
	cursor: crosshair;
}
div.sheetPane div.cursorDotReadOnly {
	background-color: #3E62A8;
/*	border: 2px solid #eff4f9; */
	width: 7px;
	height: 7px;
	z-index: 20000005;
	padding: 0px;
}
div.sheetPane div.focusBox {
	z-index: 20000003;
	border: 2px solid #3E62A8;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=80);
	opacity: 0.80;
	-moz-opacity: 0.80;
	padding: 0px;
}
div.sheetPane div.crosshairBox {
	padding: 0px;
	margin: -1px 0px 0px -1px;
	border: 2px solid #3E62A8;
	background-color: #89A6C4;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=20);
	opacity: 0.20;
	-moz-opacity: 0.20;
	z-index: 20000000;
}
div.sheetPane div.namedRangeOriginBox {
	padding: 0px;
	background-color: transparent;
	border: 2px solid #cc0000;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=50);
	opacity: 0.5;
	-moz-opacity: 0.5;
	z-index: 20000005;
}
div.sheetPane div.crosshairBoxN,
div.sheetPane div.crosshairBoxS {
}
div.sheetPane div.crosshairBoxE,
div.sheetPane div.crosshairBoxW {
}
div.sheetPane div.RTU {
	background-color: #ff2020;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=40);
	opacity: 0.40;
	-moz-opacity: 0.40;
	padding: 2px;
}
div.sheetPane div.copy {
	border: 2px dashed #3E62A8;
	padding: 0px;
}
div.sheetPane div.selector {
	border: 2px dashed #000000;
	padding: 0px;
}
div.sheetPane div.filler {
	border: 2px dashed #A4B3C4;
	padding: 0px;
}
div.sheetPane div.lock {
	border: 1px solid #000000;
	background-color: #999999;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=15);
	opacity: 0.15;
	-moz-opacity: 0.15;
	margin: -1px 0px 0px -1px;
	padding: 1px 1px 1px  1px;
}
div.sheetPane div.lockCtrl {
	background-image: url(img/LockCtrl.gif);
	width: 10px;
	height: 10px;
	padding: 0px;
	z-index: 20000004;
}
div.sheetPane div.myLock {
	border: 2px solid #b9b900;
	padding: 0px;
}
div.sheetPane div.myLockCtrl {
	background-image: url(img/MyLockCtrl.gif);
	width: 10px;
	height: 10px;
	padding: 0px;
	z-index: 20000004;
}
div.sheetPane div.cellCommentIndicatorCtrl {
	position: absolute;
	background-image: url(img/CellCommentIndicatorCtrl.gif);
	cursor: pointer;
	right: 0;
	top: 0;
	width: 10px;
	height: 10px;
	z-index: 20000006;
}
div.sheetPane div.cellErrorIndicatorCtrl {
	background-color: green;
	right: 0px;
	width: 6px;
	height: 6px;
	z-index: 20000004;
	margin-left: 4px;
}
div.sheetPane div.mergedCellBox {
	z-index: 20000002;
	margin: -1px 0px 0px -1px;
	padding: 1px 3px 1px  3px;
}
div.sheetPane div.mergedCellBox a {
	text-decoration: underline;
}
div.sheetPane div.filterFieldBox {
	z-index: 20000008;
	margin: -1px 0px 0px -1px;
}
div.sheetPane div.filterFieldBoxButton,
div.sheetPane div.filterFieldBoxButtonHover,
div.sheetPane div.filterFieldBoxButtonVisible,
div.sheetPane div.filterFieldBoxButtonActive,
div.sheetPane div.filterFieldBoxButtonActiveHover,
div.sheetPane div.filterFieldBoxButtonActiveVisible {
	right: 0px;
	bottom: 0px;
	background: url(img/FilterButton.gif) no-repeat;
	cursor: default;
}
div.sheetPane div.filterFieldBoxButton,
div.sheetPane div.filterFieldBoxButtonHover {
	background-position: -0px -0px;
}
div.sheetPane div.filterFieldBoxButtonVisible {
	background-position: -17px -0px;
}
div.sheetPane div.filterFieldBoxButtonActive,
div.sheetPane div.filterFieldBoxButtonActiveHover {
	background-position: -0px -17px;
}
div.sheetPane div.filterFieldBoxButtonActiveVisible {
	background-position: -17px -17px;
}
.cellAttachment {
	position: absolute;
	z-index: 20000006;
	border: solid 1px #404000;
	background: #ffffcc;
	width: 120px;
	height: 80px;
}

.cellPulldownButton,
.cellPulldownButtonDown {
	position: absolute;
	z-index: 20000006;
	width: 17px;
	height: 16px;
	font-size: 0.01mm;
	overflow: hidden;
