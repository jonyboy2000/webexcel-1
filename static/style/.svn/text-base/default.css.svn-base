span.BookHeadButton_comment div.ButtonIcon  {	background-position: -133px 0px }
span.BookHeadButton_chat div.ButtonIcon  {	background-position: -76px 0px }

/* Side Pane  */
.SidePane {
		font-size: 12px;
		font-family: arial, verdana;
		width: 100%; 
		height: 100%;
		margin: 0px;
		padding: 0px;
		background-color: #f3f3f3;
		overflow: hidden;
}

.SidePane .SidePaneHeader {
		background-color: #bac7d8;
		padding: 0px 5px 0px 10px;
		width: 100%;
}

.SidePane .SidePaneHeader .HeaderButton { 
		height: 21px;
		width: 100%; 
		margin: 0px;
		padding: 0px 0px 0px 5px; 
  	border-top: 2px solid #7494bb;
}

.SidePane .SidePaneHeader .Button {
		width: 21px;
		border-left: 1px solid #bac7d8;
		background-color: white;
		cursor: pointer;
}

.SidePane .SidePaneHeader .SelectedButton {
		width: 21px;
		border-left: 1px solid #bac7d8;
		background-color: #f3f3f3;
		cursor: pointer;
}

.SidePane .SidePaneHeader .Button a, .SidePane .SidePaneHeader .SelectedButton a {
		text-decoration: none;
		padding: 0px;
		margin: 0px;
		display: block;
		height: 21px;
}

.SidePane .SidePaneHeader .Button span {
		width: 21px;
		height: 21px;
		display: block;
		cursor: pointer;
		background: white url(img/sidepane/headerIcons.gif) 0px 0px no-repeat;
}

.SidePane .SidePaneHeader .SelectedButton span{
		width: 21px;
		height: 21px;
		display: block;
		cursor: pointer;
		background: #f3f3f3 url(img/sidepane/headerIcons.gif) 0px 0px no-repeat;
}

.SidePane .SidePaneHeader .Button span.CommentButton, .SidePane .SidePaneHeader .SelectedButton span.CommentButton { background-position: -0px 0px } 
.SidePane .SidePaneHeader .Button span.ChatButton, .SidePane .SidePaneHeader .SelectedButton span.ChatButton { background-position: -21px 0px }

.SidePane .SidePaneHeader .CloseButton { 
		height: 21px;
		margin: 3px 3px 0px 0px;
		cursor: pointer;
		text-align: right;
}

.SidePane .SidePaneHeader .CloseButton a.CloseButtonLink {
		float: right;
		display: block;
		height: 21px;
		width: 21px;
		background: #bac7d8 url(img/sidepane/headerIcons.gif) 0px 0px no-repeat;
		background-position: -63px 0px;
		cursor: pointer;
}

.SidePane .SidePaneHeader .CloseButton a.CloseButtonLink:hover {
		display: block;
		height: 21px;
		width: 21px;
		background: #bac7d8 url(img/sidepane/headerIcons.gif) 0px 0px no-repeat;
		background-position: -42px 0px;
}

.SidePane .Title {
		color: #369; 
		font-weight: bold; 
		font-size: 12px; 
		padding: 3px 3px 3px 8px;
		background-color: #f3f3f3;
}

.SidePane .Content{
