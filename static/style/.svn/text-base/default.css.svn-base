	width: 100%;
	margin-top: 100px;
	font-size: 10px;
	color: #888;
}

.MacroEmptyBox td {
	padding: 0px 50px 0px 50px;
}

.Selected .MacroItemBox {
	border-color: #dce4ed;
	background: #eff4f9;
}

.MacroList li a {
	border: none;
}

.MacroList li a:hover .MacroItemBox {
	border-color: #dce4ed;
	background: #eff4f9;
}

.MacroItemBox table {
	background: inherit;
	color: inherit;
	padding: 8px;
	width: 100%;
}

.MacroItemBox td {
	vertical-align: top;
}

.MacroIcon {
	background: url(img/MacroIcon.gif) no-repeat;
	width: 36px;
	height: 36px;
	margin: 4px;
}

.MacroTitle {
	font-weight: bold;
	color: #333;
	font-size: 12px;
	padding: 3px;
}

.MacroRemark, .MacroShortDesc, .MacroLongDesc, .MacroViewSource {
	font-weight: normal;
	font-size: 10px;
}

.MacroRemark, .MacroShortDesc, .MacroLongDesc {
	color: #555;
}

.MacroShortDesc, .MacroLongDesc, .MacroViewSource {
	padding: 3px 3px 3px 10px;
}

.MacroViewSource {
	color: #3399CC;
}

.MacroLongDesc a div {
	padding: 4px;
}

.MacroLongDesc a:hover div {
	background: #ffffce;
}

.MacroSource {
	padding: 5px;
	background: white;
	overflow: auto;
	font-size: 12px;
	border: 1px solid #e0e0e0;

}

.Selected .MacroLongDesc, .Selected .MacroViewSource, .MacroShortDesc {
	display: block;
}

.Selected .MacroShortDesc, .MacroLongDesc, .MacroViewSource {
	display: none;
}

.Disabled .MacroItemBox,
.Disabled .MacroRemark,
.Disabled .MacroShortDesc,
.Disabled .MacroLongDesc,
.Disabled .MacroTitle {
	color: #888
}

.MacroControl {
	padding: 2px;
	display: none;
}

.MacroControl button {
	margin-right: 1px;
	margin-left: 1px;
	margin-bottom: 0px;
	margin-top: 0px;
}

.MacroControl .EnableButton,
.Disabled .MacroControl .DisableButton {
	display: none;
}

.Disabled .MacroControl .EnableButton,
.MacroControl .DisableButton {
	display: inline;	
}

.Selected .MacroControl {
	display: block;
}
