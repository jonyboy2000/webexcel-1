/* HTML Elements */

* {
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-khtml-box-sizing: border-box;
}

body {
	margin: 0;
	padding: 0;
	font-size: 13px;
	background: white;
	width: 100%;
	height: 100%;
	position: absolute;
}

body, input, select, button {
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

* {
	font-size: 100%;
}

table {
	border-collapse: collapse;
}

form {
	display: inline;
}

a {
	color: #3366CC;
	text-decoration: none;	
}
a:hover {
	color: #CC6666;
	text-decoration: underline;
}
a:active {
	color: #EE7766;
	text-decoration: underline;
}

img {
	display: block;
	border: 0;
}

ul {
	list-style-type: square;
}

li {
	margin-top: 5px;
	margin-bottom: 5px;
}

h1, h2, h3, h4, h5, h6 {
	font-weight: bold;
}

h1 {
	font-size: 22px;
	margin: 16px 0px;
}

h2 {
	font-size: 18px;
	margin: 10px 0px;
}

h3 {
	font-size: 15px;
	margin: 10px 0px;
}

h4 {
	font-size: 12px;
}

/* General Classes */

a.Block {
	display: block;
	width: 100%;
	height: 100%;
}

.Error {
	color: red;
}

.Comment {
	color: gray;
}

.FootNote {
		color: gray;
		font-size: 10px;
}

.Blue {
	color: blue;
}

.CenterAlign {
		text-align: center;
}

.LeftAlign { 
		text-align: left;
}

.RigthAlign {
		text-align: right;
}

.JustifyText {
		text-align: justify;
}

.CenterPosition {
		margin-left: auto !important;
		margin-right: auto !important;
}

.PointerCursor {
		cursor: pointer;
}

.Inline {
	display: inline;
}

.NoPad {
	padding: 0;
}

.NoWrap {
	white-space: nowrap;
}

.RedAlertBox {
		background-color: #FBEBEB;
		border: 1px solid #FADADA;
		color: #CC6666;
		padding: 10px 20px;
}

.RedAlertBox .Title {
		color: #CC3333;
		font-weight: bold;
		text-decoration: underline;
		padding-bottom: 10px;
}

.RedAlertBox a {
		color: #600;
}

.BlueAlertBox {
		background-color: #EFF4F9;
		border: 1px solid #DCE4ED;
		color: #5181a6;
		padding: 10px 20px;
}

.BlueAlertBox .Title {
		color: #336699;
		font-weight: bold;
		text-decoration: underline;
		padding-bottom: 10px;
}


.Small {
	font-size: 11px;
}

ul.Plain {
	padding: 0;
	margin: 0;
	list-style: none;
}

.Loading {
	height: 16px;
	width: 16px;
	background: url(img/ProgressBarLoader.gif) center right no-repeat;
}

.Bold {
  font-weight: bold;
}

.Underline {
  text-decoration: underline;
}

.LineThrough {
  text-decoration: line-through;
}

img.HelpIcon {
		display: inline;
		margin-left: 3px;
		vertical-align: text-bottom;
}

div.Label {
	border-style: solid;
	border-width: 1px;
	padding: 2px 4px;
}

div.LabelDark {
	border-color: #303030;
	background: #d0d0d0;
	color: #303030;
}

div.Button {
	padding-left: 4px;
	background-image: url(img/Button.gif);
	background-position: 0% 100px;
	background-repeat: no-repeat;
	cursor: default;
}
div.Button div.ButtonTail {
	background-image: url(img/Button.gif);
	background-position: 0% 100px;
	background-repeat: no-repeat;
}
div.Button table {
	height: 20px;
	font-size: 100%;
	border-collapse: collapse;
}
div.Button table td {
	padding-right: 4px;
	white-space: nowrap;
}
div.Button table td * {
	vertical-align: middle;
}
div.ButtonHover {
	background-position: 0% -0px;
}
div.ButtonHover div.ButtonTail {
	background-position: 100% -100px;
}
div.ButtonDown {
	background-position: 0% -200px;
}
div.ButtonDown div.ButtonTail {
	background-position: 100% -300px;
}
div.ButtonLink {
	cursor: pointer;
	color: black;
}

/* Specific Layout Classes */

table.PageHead {
	border-spacing: 0px;
	border-collapse: collapse;
	height: 44px;
	width: 100%;
	background: #336B97;
}
td.PageHeadTitle, td.PageHeadUser, td.PageHeadTab, td.PageHeadDropdown {
	white-space: nowrap;
	padding: 0px;
}
td.PageHeadUser {
		background-color: #336B97;
}
div.PageHeadUser{
	position: relative;
	top: 7px;
	width: 270px;
	font-size: 11px;
	line-height: 15px;
	padding-left: 5px;
}
.PageHeadUser, .PageHeadUser a{
		font-family: arial;
		color: white;
}
.PageHeadUser a:hover {
	color: white;
}

.PageHeadUser a.UserModeIndicator {
		color: #ffcc33;
}

.PageHeadUserMessage {
	padding: 0; 
}

.PageHeadDropdownButton {
	display: block;
	width: 15px;
	height: 19px;
	margin: 0px 3px;
	background: url(img/PageHeadDrop.gif) no-repeat center left;
}
.PageHeadDropdownButton:hover {
	background-position: center right;
}

.PageTab {
	font-family: Arial, Verdana;
	font-size: 13px;
	font-weight: bold;
}
.PageTab a {
		font-weight: bold;
}
table.PageTab {
 	background-color: #336B97;
}
.PageTab td {
	padding: 0;
	vertical-align: middle;
}
.PageTabSpacer {
	border-right: 2px solid #336B97;
}
.PageTabActive, .PageTabInactive {
		padding: 5px 10px;
}
.PageTabActive {
	background-color: #FFFFFF;
}
.PageTabInactive {
  background-color: #5181a6;
}
.PageTabInactive_Signup {
	background-color: #FFDD3F;
}
.PageTabInactive_Signup a:hover {
		text-decoration: none;
		background-color: #FFEB8F;
}
.PageTab td.Text a {
	display: block;
	padding: 4px 18px 3px 17px;
	white-space: nowrap;

}
.PageTab td.Text a.HasDrop {
	padding-right: 5px;
}
.PageTab td.Text a, .PageTab .PageTabActive a:hover {
	color: white;
}
.PageTab .PageTabInactive a:hover {
	background-color: #7da1be;
	text-decoration: none;
}

.PageTab .PageTabActive td.Text a, .PageTab .PageTabActive td.Text a:hover, .PageTab .PageTabInactive_Signup td.Text a {
	color: #369;
	text-decoration: none;
}
.PageTab td.Drop {
	padding-left: 1px;
	padding-right: 10px;
}
.PageTab td.Drop a {
	display: block;
	position: relative;
	top: 2px;
}
.PageTab td.Drop a img {
	background-image: url(img/PageTabDrop.gif);
	background-repeat: no-repeat;
	width: 15px;
	height: 11px;
}
.PageTab .PageTabInactive td.Drop a img {
	background-position: bottom left;
}
.PageTab .PageTabInactive td.Drop a:hover img {
	background-position: bottom right;
}
.PageTab .PageTabActive td.Drop a img {
	background-position: top left;
}
.PageTab .PageTabActive td.Drop a:hover img {
	background-position: top right;
}

table.FooterLinksTable {
		width: 95%;
		font-family: arial, Verdana;
		font-size: 11px;
		margin: 0px 15px 0px 23px;
		border-left: 1px solid #e8e8e8;
		border-right: 1px solid #e8e8e8;
}
	
