A.PerspectiveButton {
	display: block;
	width: 22px;
	height: 22px;
	background: url(img/PerspectiveButton.gif) 0px 0px no-repeat;
}
a.PerspectiveButton:hover {
	background-position: -24px 0px;
}
a.PerspectiveButton:active {
	background-position: -48px 0px;
}
a.PerspectiveButton.Active, a.PerspectiveButton.Active:hover {
	background-position: -71px 0px;
}

a.PerspectiveButton span{
	display: block;
	position: relative;
	top: 1px;
	left: 1px;
	width: 19px;
	height: 19px;
	background: url(img/PerspectiveIcons.gif) 0px 0px no-repeat;
}
a.PerspectiveButton_grid span {	background-position: -0px 0px }
