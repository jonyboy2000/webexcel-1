}

table.FooterLinksTable table.ColumnTable td{
		padding: 0px;
		padding-left: 15px;
		margin-left: 5px;
		white-space: nowrap;
}
table.FooterLinksTable table.ColumnTable tr{
		height: 10px;
}

.EmailPreview {
	font-size: 11px;
	border: 1px dotted #CCC;
	padding: 8px; 
	background: #FCFCFC;
}

.EmailPreview #TogglerLink {
