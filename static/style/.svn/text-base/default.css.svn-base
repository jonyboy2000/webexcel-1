.scrollbar {
	position: relative;
	background-color: #e6e6e6;
	-moz-user-focus: normal;
	-moz-user-select: none;
	-khtml-user-select: none;
	user-select: none;
	cursor: default;
}

.hori {
	width: 300px;
	height: 17px;
}

.vert {
	width: 17px;
	height: 300px;
}

.scrollbar .handle {
	position: absolute;
	font-size: 1px;
	-moz-user-select: none;
	-khtml-user-select: none;
	user-select: none;
	cursor: default;
	border: 1px solid;
	background: #d4d0c8;
	border-color: #d4d0c8 #404040 #404040 #d4d0c8;
	border-collapse: separate;
}

.scrollbar .handle, .scrollbar .handle td {	
	padding: 0px;
	margin: 0px;
}

.scrollbar .handle td {
	border: 1px solid;
	border-color: #ffffff #808080 #808080 #ffffff;
}

.scrollbar.hori .handle {
	width: 31px;
	height: 16px;
}

.scrollbar.vert .handle {
	width: 16px;
	height: 31px;
}

.scrollbar .track {
	position: absolute;
	font-size: 0.01mm;
	overflow: hidden;
	background: url(img/ScrollbarBg.gif);
	width: 1px;
	height: 1px;
}

.scrollbar.vert .track {
	width: 17px;
	border-right: 1px solid #d4d0c8;
}

.scrollbar.hori .track {
	height: 17px;
	border-bottom: 1px solid #d4d0c8;
}

.scrollbar .track div {
	width: 100%;
	height: 100%;
